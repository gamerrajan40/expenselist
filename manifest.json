{
  "manifest_version": 2,
  "name": "Track Expense",
  "short_name": "Expense",
  "version": "1.1",
  "description": "Track your expense. Completly offline.",
  "icons": {
    "16": "icons/expense16.png",
    "48": "icons/expense48.png",
    "128": "icons/expense128.png"
  },
  "browser_action": {
    "default_icon": "icons/expense16.png",
    "default_popup": "expenselist.html"
  },

  "permissions": ["storage", "notifications"]
}
